								"exec": [
									"const utilityPkg = pm.require('@team03lmsapi/utility-pkg');\r",
									"const responseBody = pm.response.json();\r",
									"const programId = responseBody.programId;\r",
									"pm.globals.set(\"programId\", programId);\r",
									"\r",
									"const programName = responseBody.programName;\r",
									"pm.globals.set(\"programName\", programName);\r",
									"\r",
									"pm.test(\"Status code is 201\", function () {\r",
									"    pm.response.to.have.status(201);\r",
									"pm.test(\"Content-Type is present\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"});\r",
									"});\r",
									"pm.test(\"Successful POST request\", function () {\r",
									"    pm.expect(pm.response.code).to.be.oneOf([201, 202]);\r",
									"});\r",
									""
