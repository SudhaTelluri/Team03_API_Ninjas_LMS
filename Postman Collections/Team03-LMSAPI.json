					"event": [
						{
							"listen": "test",
							"script": {
								"id": "1c340750-a5e8-4b14-befa-62d0495e2dcf",
								"exec": [
									"const utilityPkg = pm.require('@team03lmsapi/utility-pkg');\r",
									"const responseBody = pm.response.json();\r",
									"const programId = responseBody.programId;\r",
									"pm.globals.set(\"programId\", programId);\r",
									"\r",
									"const programName = responseBody.programName;\r",
									"pm.globals.set(\"programName\", programName);\r",
									"\r",
									"pm.test(\"Status code is 201\", function () {\r",
									"    pm.response.to.have.status(201);\r",
									"pm.test(\"Content-Type is present\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"});\r",
									"});\r",
									"pm.test(\"Successful POST request\", function () {\r",
									"    pm.expect(pm.response.code).to.be.oneOf([201, 202]);\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {
									"@team03lmsapi/utility-pkg": {
										"id": "0192ff11-ef23-7186-96cb-b4d0992c81db"
									}
								}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"id": "fd022429-a8a3-4026-89c0-b811f4c3c4c9",
								"exec": [
									"const teamName = \"APINinjas\";\r",
									"\r",
									"// Generate a random 4-letter suffix for uniqueness\r",
									"const letters = \"ABCDEFGHIJKLMNOPQRSTUVWXYZ\";\r",
									"let suffix1 = \"\", suffix2 = \"\";\r",
									"for (let i = 0; i < 4; i++) {\r",
									"    suffix1 += letters.charAt(Math.floor(Math.random() * letters.length));\r",
									"    suffix2 += letters.charAt(Math.floor(Math.random() * letters.length));\r",
									"}\r",
									"\r",
									"// Generate unique program names for two programs\r",
									"const programName1 = `${teamName}-${suffix1}`; // e.g., APINinjas-ABCD\r",
									"const programName2 = `${teamName}-${suffix2}`; // e.g., APINinjas-EFGH\r",
									"\r",
									"// Descriptions for each program\r",
									"const programDescription1 = `Program ${programName1} for team ${teamName}`;\r",
									"const programDescription2 = `Program ${programName2} for team ${teamName}`;\r",
									"\r",
									"// Set program status\r",
									"const programStatus = \"Active\"; \r",
									"\r",
									"// Set values in environment variables for use in requests\r",
									"pm.environment.set(\"programName1\", programName1);\r",
									"pm.environment.set(\"programDescription1\", programDescription1);\r",
									"pm.environment.set(\"programName2\", programName2);\r",
									"pm.environment.set(\"programDescription2\", programDescription2);\r",
									"pm.environment.set(\"programStatus\", programStatus);\r",
									"\r",
									"// Log generated values for verification\r",
									"console.log(\"Generated Program 1 Name:\", programName1);\r",
									"console.log(\"Generated Program 2 Name:\", programName2);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
